                                 .equ line1 = 0x00 ; Labels for setting the LCD cursor to write on the correct line.
                                 .equ line2 = 0x40
                                 
                                 .cseg
                                 .org 0
                                 
                                 ; (R30,R31) is the Z register, for accessing the words in program memory.
                                 ; (R28,R29) is the Y register, which temporarily stores the state of the Z register for returning to.
                                 ; R0 is the register used when the send_byte subroutine is called.
                                 ; R27 is the counter used to track what word is being displayed.
                                 ; R26 is the register that stores the command for the send_command subroutine.
                                 ; R26 is the button state register - bit 0 is the current button state, bit 1 is the previous button state.
                                 
                                 ; R16, R17 are temporary registers and will not preserve their value across a subroutine call (Used by send_byte, delay_100us, etc.).
                                 
                                 words: ; Spaces are included so that the byte count is even, since we are working in a 16-bit instruction processor
000000 7250
000001 7365
000002 2073
000003 6f74
000004 5320
000005 7263
000006 6c6f
000007 006c                      	.DB "Press to Scroll",0x00 ; 16 bytes, This one should not reappear in the scrolling sequence
                                 words_loop: ; Point to send Z to on looping.
000008 7241
000009 6974
00000a 6966
00000b 6963
00000c 6c61
00000d 0020                      	.DB "Artificial ",0x00 ; 12 bytes
00000e 6e49
00000f 6574
000010 6c6c
000011 6769
000012 6e65
000013 6563
000014 0020                      	.DB "Intelligence ",0x00 ; 14 bytes
000015 6f52
000016 6f62
000017 6974
000018 7363
000019 0020                      	.DB "Robotics ",0x00 ; 10 bytes
00001a 614d
00001b 6863
00001c 6e69
00001d 0065                      	.DB "Machine",0x00 ; 8 bytes
00001e 654c
00001f 7261
000020 696e
000021 676e
000022 0020                      	.DB "Learning ",0x00 ; 10 bytes
000023 654e
000024 7275
000025 6c61
000026 0020                      	.DB "Neural ",0x00 ; 8 bytes
                                 ; The 0x00 following each string is a null character being added as the final byte, making the strings even and serving as a sentinel character like in C.
                                 ; There are 7 words, including the "Press to Scroll" statement. Therefore the index will start at 0, and loop to 1 after going to 6.
                                 
                                 init:
000027 9a23                      	sbi DDRB,3 ; pb5 (enable) is output
000028 9a25                      	sbi DDRB,5 ; pb3 (register select) is output
000029 9a38                      	sbi DDRC,0 ; \
00002a 9a39                      	sbi DDRC,1 ; | data lines
00002b 9a3a                      	sbi DDRC,2 ; | are output
00002c 9a3b                      	sbi DDRC,3 ; /
00002d 9a3d                      	sbi DDRC,5 ; pc5 (LED) is output
00002e 9852                      	cbi DDRD,2 ; pd2 (Pushbutton) is input
                                 
00002f 9a45                      	sbi PORTC,5 ; Turn LED off until initialization is complete.
                                 
000030 e0b0                      	ldi R27,0 ; Set the string counter to 0.
000031 e0e0                      	ldi R30,LOW(2*words) ; Set the Z-register to the "words" label.
000032 e0f0                      	ldi R31,2*HIGH(words)
                                 
                                 	; Initializing the LCD Display
000033 d075                      	rcall delay_100ms ; Wait 0.1 seconds before sending commands to the LCD.
                                 
000034 982d                      	cbi PORTB,5 ; Setting the LCD to command mode.
                                 
000035 b106                      	in R16,PINC
000036 7f00                      	cbr R16,0x0F
000037 7003                      	andi R16,0x03
000038 b908                      	out PORTC,R16 ; Send 8-bit command to LCD
000039 d057                      	rcall LCD_strobe
00003a d069                      	rcall delay_5ms ; Wait 5 ms.
                                 
00003b b106                      	in R16,PINC
00003c 7f00                      	cbr R16,0x0F
00003d 7003                      	andi R16,0x03
00003e b908                      	out PORTC,R16 ; Send 8-bit command to LCD
00003f d051                      	rcall LCD_strobe
000040 d058                      	rcall delay_100us ; Wait 200 us.
000041 d057                      	rcall delay_100us
                                 
000042 b106                      	in R16,PINC
000043 7f00                      	cbr R16,0x0F
000044 7003                      	andi R16,0x03
000045 b908                      	out PORTC,R16 ; Send 8-bit command to LCD
000046 d04a                      	rcall LCD_strobe
000047 d051                      	rcall delay_100us ; Wait 200 us.
000048 d050                      	rcall delay_100us
                                 
000049 b106                      	in R16,PINC
00004a 7f00                      	cbr R16,0x0F
00004b 7002                      	andi R16,0x02
00004c b908                      	out PORTC,R16 ; Send 4-bit command to LCD
00004d d043                      	rcall LCD_strobe
00004e d04a                      	rcall delay_100us ; Wait 200 us.
00004f d049                      	rcall delay_100us
                                 
000050 9845                      	cbi PORTC,5 ; Once initialization is complete, turn LED back on.
                                 
000051 c008                      	rjmp update_text ; Write the initial text to the display.
                                 
                                 loop:
                                 	; Button logic:
000052 0f99                      	lsl R25 ; Update past button state (bit 2)
000053 7092                      	andi R25,0x02 ; Don't let the other 6 bits get set
000054 9b4a                      	sbis PIND,2 ; If button is pressed (PD2 is pulled low), set bit 0 of R25 to 1. Otherwise it will be 0 due to the logical shift left.
000055 6090                      	sbr R25,0 ; Set bit 0 to 1.
                                 
000056 3091                      	cpi R25,0x01 ; Check if button was just pressed (button is pressed now but was not pressed on the last loop).
000057 f011                      	breq update_text ; If so then update the text accordingly.
000058 d04b                      	rcall delay_5ms ; Otherwise wait 5 milliseconds before checking again (acts as a debounce).
000059 cff8                      	rjmp loop
                                 
                                 ; To make the first line display what was previously on the second line, we will on button press do:
                                 ; Clear LCD; Set LCD pointer to 0x00 (Line 1); run display_line; run cycle_words; Set LCD pointer to 0x40 (Line 2); run display_line; move Z back to begining of line.
                                 
                                 ; Runs on button press, Not a subroutine (accessed through branch statement in loop)
                                 update_text:
00005a e0a1                      	ldi R26,0x01 ; Send the Clear Display command to LCD.
00005b d01c                      	rcall send_command
                                 
00005c e8a0                      	ldi R26,0x80 ; Set the DDRAM pointer to 0x00 (Start of line 1)
00005d d01a                      	rcall send_command
                                 
00005e d012                      	rcall display_line ; Display line 1
00005f d008                      	rcall cycle_words
                                 
000060 eca0                      	ldi R26,0xC0 ; Set the DDRAM pointer to 0x40 (Start of line 2)
000061 d016                      	rcall send_command
                                 
000062 2fce                      	mov R28,R30 ; Store start of second line to return to
000063 2fdf                      	mov R29,R31
000064 d00c                      	rcall display_line ; Display line 2
000065 2fec                      	mov R30,R28 ; Set Z to what it was before now, so we can output this again next time.
000066 2ffd                      	mov R31,R29
000067 cfea                      	rjmp loop
                                 
                                 cycle_words:
000068 30b6                      	cpi R27,6
000069 f019                      	breq _cycle_back ; If the word index is at 6, cycle back to 0 instead of incrementing.
00006a 95b3                      	inc R27 ; Advance the word index counter.
00006b 9631                      	adiw Z,1 ; Advance the Z-index past the null character to the next word.
00006c 9508                      	ret ; Exit cycle_words subroutine (without running _cycle_back).
                                 _cycle_back:
00006d e0b1                      	ldi R27,1 ; Set the word index counter to 1.
00006e e1e0                      	ldi R30,LOW(2*words_loop) ; Move the Z-index to the first word in the loop.
00006f e0f0                      	ldi R31,2*HIGH(words_loop)
000070 9508                      	ret ; Exit cycle_words subroutine.
                                 	
                                 
                                 ; Sends the text pointed to by the Z-register to the display.
                                 display_line:
000071 9a2d                      	sbi PORTB,5 ; Ensure that we are in text mode, not command mode.
000072 9005                      	lpm R0,Z+ ; Load byte into R0.
000073 2000                      	tst R0 ; Does R0 = 0x00 (Null character)?
000074 f011                      	breq _end ; If so then exit the loop.
000075 d009                      	rcall send_byte
000076 cffa                      	rjmp display_line ; Loop back to start
                                 _end:
000077 9508                      	ret ; Exit display_line
                                 
                                 ; Sends a command stored in R26 to the display.
                                 send_command:
000078 982d                      	cbi PORTB,5 ; Set the LCD to command mode
000079 2e0a                      	mov R0,R26
00007a d004                      	rcall send_byte
00007b 30a4                      	cpi R26,0x04 ; If command is not Clear Display or Cursor Home, end here.
00007c f7d0                      	brsh _end
00007d d026                      	rcall delay_5ms ; Otherwise wait 5 ms to exceed the 1.64 ms safety margin.
00007e 9508                      	ret
                                 
                                 ; Sends the byte stored in R0 to the display, either for commands or for text.
                                 send_byte:
00007f 2d00                      	mov R16,R0 ; Send the upper nibble of R0 first.
000080 9502                      	swap R16
000081 700f                      	cbr R16,0xF0
000082 b116                      	in R17,PINC ; Place the upper nibble of PORTC into R17.
000083 7f10                      	cbr R17,0x0F
000084 2301                      	and R16,R17 ; Combine R17 upper nibble and R16 lower nibble.
000085 b908                      	out PORTC,R16
000086 d00a                      	rcall LCD_strobe
000087 d011                      	rcall delay_100us
                                 	
000088 2d00                      	mov R16,R0 ; Then send the lower nibble of R0
000089 700f                      	cbr R16,0xF0
00008a b116                      	in R17,PINC ; Place the upper nibble of PORTC into R17.
00008b 7f10                      	cbr R17,0x0F
00008c 2301                      	and R16,R17 ; Combine R17 upper nibble and R16 lower nibble.
00008d b908                      	out PORTC,R16
00008e d002                      	rcall LCD_strobe
00008f d009                      	rcall delay_100us
000090 9508                      	ret
                                 
                                 ; Sets the Enable line from low to high, waits 250 ns, then clears the Enable line back to low.
                                 LCD_strobe:
000091 982b                      	cbi PORTB,3 ; Set Enable to low.
000092 9a2b                      	sbi PORTB,3 ; Set Enable to high.
000093 0000                      	nop ; 4 NOP instructions so at least 230 nanoseconds pass (about 250 ns should pass).
000094 0000                      	nop
000095 0000                      	nop
000096 0000                      	nop
000097 982b                      	cbi PORTB,3 ; Set Enable back to low
000098 9508                      	ret
                                 
                                 ; Note: at 16 MHz, 1 clock cycle is 0.0625 microseconds (0.0625 us = 62.5 ns)
                                 ; Therefore, 100 us is 1600 clock cycles.
                                 
                                 ; Approximately 100 us delay (1606 cycles).
                                 delay_100us:
000099 ef0f                      	ldi r16,255 ; 5*255 + 5*65 = 1600 cycles
                                 	_d1:
00009a 0000                      		nop
00009b 0000                      		nop
00009c 950a                      		dec r16
00009d f7e1                      		brne _d1
00009e e401                      	ldi r16,65
                                 	_d2:
00009f 0000                      		nop
0000a0 0000                      		nop
0000a1 950a                      		dec r16
0000a2 f7e1                      		brne _d2
0000a3 9508                      	ret
                                 
                                 ; Approximately 5 ms delay.
                                 delay_5ms:
0000a4 e312                      	ldi r17,50
                                 	_d3:
0000a5 dff3                      		rcall delay_100us
0000a6 951a                      		dec r17
0000a7 f7e9                      		brne _d3
0000a8 9508                      	ret
                                 
                                 ; Approximately 100 ms delay.
                                 delay_100ms:
0000a9 e124                      	ldi r18,20
                                 	_d4:
0000aa dff9                      		rcall delay_5ms
0000ab 952a                      		dec r18
0000ac f7e9                      		brne _d4
0000ad 9508                      	ret


